#!/bin/bash

source build/common

export ADAL_TRAVIS_BUILD_TASK="Mac_32bit_Framework"

echo "==========================[Building Mac OS X Framework (32-bit)]=========================="

# Build and test the Mac OS X ADAL Framework
xcodebuild test -workspace ADAL.xcworkspace -scheme "ADAL Mac" CURRENT_ARCH=i386 | $ADAL_XCPRETTY

RETVAL=$?

exit $RETVAL

